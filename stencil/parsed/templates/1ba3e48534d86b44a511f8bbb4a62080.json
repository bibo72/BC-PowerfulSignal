{"components/amp/common/footer":"<footer class=\"amp-footer\" role=\"contentinfo\">\r\n    <article data-section-type=\"footer-webPages\">\r\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.navigate'}}</h5>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each pages}}\r\n            <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n            <li>\r\n                <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n            </li>\r\n        </ul>\r\n    </article>\r\n\r\n    <article data-section-type=\"footer-categories\">\r\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.categories'}}</h5>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each categories}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </article>\r\n\r\n    <article data-section-type=\"footer-brands\">\r\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.brands'}}</h5>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each shop_by_brand}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n            {{/each}}\r\n            <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n        </ul>\r\n    </article>\r\n\r\n    <article data-section-type=\"storeInfo\">\r\n        <h5 class=\"amp-footer-heading\">{{lang 'footer.info'}}</h5>\r\n        <address>{{nl2br settings.address}}</address>\r\n        {{#if settings.phone_number}}\r\n            <strong>{{lang 'footer.call_us' phone_number=settings.phone_number}}</strong>\r\n        {{/if}}\r\n    </article>\r\n    <article>\r\n        {{#if theme_settings.show_powered_by}}\r\n            <p>Powered by <a href=\"http://bigcommerce.com\" rel=\"nofollow\">BigCommerce</a></p>\r\n        {{/if}}\r\n        {{#if theme_settings.show_copyright_footer}}\r\n            <p class=\"powered-by\">&copy; {{settings.store_name}} </p>\r\n        {{/if}}\r\n    </article>\r\n\r\n<!-- Google Analytics Dynamic Remarketing\r\n    <script>\r\n    if(window.location.pathname == \"/\"){\r\n                var google_tag_params = {\r\n                    ecomm_pagetype: 'home'\r\n                };\r\n    }\r\n    else if (document.getElementsByClassName('productView').length > 0){\r\n    var id = document.querySelector('[name=\"product_id\"]').getAttribute('value');\r\n    var price = parseFloat(document.getElementsByClassName('price')[0].innerText.replace(/[^0-9.]/g,''));\r\n                var google_tag_params = {\r\n                    ecomm_prodid: id,\r\n                    ecomm_pagetype: 'product',\r\n                    ecomm_totalvalue: price\r\n                };\r\n    }\r\n    else if (document.getElementsByClassName('subcategory-list').length > 0){\r\n                var google_tag_params = {\r\n                    ecomm_pagetype: 'category'\r\n                };\r\n    }\r\n    else if (window.location.href.indexOf('confirm') > 0){\r\n                var google_tag_params = {\r\n                    ecomm_pagetype: 'purchase'\r\n                };\r\n    }\r\n     else if(window.location.href.indexOf('/cart.php') != -1){\r\n       var id = [];\r\n    for(i=0; i<document.getElementsByClassName('cart-item-image').length; i++){\r\n     id.push(document.getElementsByClassName('cart-item-image')[i].getAttribute('src').split('products')[1].split('/')[1]);\r\n    }\r\n    var price = parseFloat(document.getElementsByClassName('cart-total-grandTotal')[0].innerText.replace(/[^0-9.]/g,''));\r\n     var google_tag_params = {\r\n                    ecomm_prodid: id,\r\n                    ecomm_pagetype: 'cart',\r\n                    ecomm_totalvalue: price\r\n                };  \r\n         }\r\n    else{\r\n                var google_tag_params = {\r\n                    ecomm_pagetype: 'other'\r\n                };\r\n    }\r\n    </script>\r\n    \r\n    <script type=\"text/javascript\">\r\n    /* <![CDATA[ */\r\n    var google_conversion_id = 1061450104;\r\n    var google_custom_params = window.google_tag_params;\r\n    var google_remarketing_only = true;\r\n    /* ]]> */\r\n    </script>\r\n    <script type=\"text/javascript\" src=\"//www.googleadservices.com/pagead/conversion.js\">\r\n    </script>\r\n    <noscript>\r\n    <div style=\"display:inline;\">\r\n    <img height=\"1\" width=\"1\" style=\"border-style:none;\" alt=\"\" src=\"//googleads.g.doubleclick.net/pagead/viewthroughconversion/1061450104/?guid=ON&amp;script=0\"/>\r\n    </div>\r\n    </noscript> -->\r\n\r\n</footer>\r\n"}
